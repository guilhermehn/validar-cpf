{
  "name": "validar-cpf",
  "version": "2.1.2",
  "description": "Validador de CPF",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "jest --coverage",
    "coveralls": "npm test --  --coverageReporters=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/guilhermehn/validar-cpf.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "cpf",
    "validar",
    "validador"
  ],
  "author": "Guilherme Nagatomo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guilhermehn/validar-cpf/issues"
  },
  "homepage": "https://github.com/guilhermehn/validar-cpf",
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "gerar-cpf": "^2.0.3",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es6": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "rules": {
      "indent": ["error", "tab"],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"]
    }
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "prettier --write", "npm test"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
