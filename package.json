{
  "name": "validar-cpf",
  "version": "2.1.2",
  "description": "Validador de CPF",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/guilhermehn/validar-cpf.git"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "cpf",
    "validar",
    "validador"
  ],
  "author": "Guilherme Nagatomo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guilhermehn/validar-cpf/issues"
  },
  "homepage": "https://github.com/guilhermehn/validar-cpf",
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gerar-cpf": "^2.0.3",
    "husky": "^4.0.1",
    "lint-staged": "^13.1.0",
    "mocha": "^10.2.0",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es6": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "npm t"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
